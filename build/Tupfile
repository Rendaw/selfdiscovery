include_rules

SOURCES = ../main.cxx ../shared.cxx ../blank.cxx ../version.cxx ../flag.cxx ../platform.cxx ../location.cxx ../program.cxx ../cxxcompiler.cxx ../clibrary.cxx

: foreach $(SOURCES) |> !object |> %B.o {objects}
